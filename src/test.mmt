model diffusion: x in R^n, d in 1, t in R -> y in R {
    partial(y, t) = d * div(grad(y, x), x)
}
    
model logistic_growth: r in 1, k in 1, t in R -> u in R {
    partial(u, t) = r * u * (1 - u / k)
}
  
model fishers: -> y in R {
let x = [0, 1],
    t = [0, 10],
    D = 1,
    r = 1,
    k = 1 in
    y = diffusion(x, D, t) + logistic_growth(r, k, t)
}
