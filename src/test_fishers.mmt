function diffusion( x -> Omega, d -> NonNegative, y(x) ) { return d * div(grad(y, x), x) }
function logistic_growth(r -> NonNegative, k -> NonNegative, u(t) ) { return r * u * (1 - u / k) }

model fishers(
    x -> [0 ... 1],
    t -> [0 ... 10],
    y(x, t)
) {
    let y_diffusion = diffusion(x=x, d=1, y=y)
    let y_growth = logistic_growth(r=1, k=1, u=y)
    dot(y) = dot(y_diffusion) + dot(y_growth)
}
